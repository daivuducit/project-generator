{
  "name": "project-generator",
  "displayName": "Project Generator",
  "publisher": "f-vscode",
  "description": "Tools for generate any templates code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:.vsc_success"
  ],
  "extensionDependencies": [
    "vscjava.vscode-spring-initializr",
    "vscjava.vscode-java-pack",
    "vmware.vscode-boot-dev-pack"
  ],
  "reccommendations": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "project-generator.newProject",
        "title": "Project Generator: New Project..."
      },
      {
        "command": "project-generator.updateTemplates",
        "title": "Project Generator: Update Templates Storage"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "project-generator.newProject"
        }
      ],
      "explorer/context": [
        {
          "command": "project-generator.newProject",
          "group": "navigation"
        },
        {
          "command": "project-generator.openUI",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2"
  }
}
